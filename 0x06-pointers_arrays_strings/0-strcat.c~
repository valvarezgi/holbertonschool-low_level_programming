#include "holberton.h"

/**
 * _strcat - Function that concatenates two strings
 * @dest: string
 * @src: string
 * Return: a pointer to the result string dest
 */

char *_strcat(char *dest, char *src)
{
	int i, j;

	i = j = 0;
	
	while (src[i] != '\0')
		i++;

	while ((src[i++] = dest[j++]) != '\0')
		
		return(dest);

}
